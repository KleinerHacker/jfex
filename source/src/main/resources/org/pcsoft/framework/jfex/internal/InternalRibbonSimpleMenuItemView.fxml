<fx:root type="HBox" fx:id="pnlRoot" styleClass="ribbon-simple-menu-item" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pcsoft.framework.jfex.ribbon.component.internal.InternalRibbonSimpleMenuItemView" onMouseClicked="#onActionMouseClick">
   <children>
      <ImageView fx:id="imgIcon" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"/>
      <Label fx:id="lblTitle" text="Title">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
   </children>
</fx:root>
