<?xml version="1.0" encoding="UTF-8"?>


<?import org.pcsoft.framework.jfex.component.OverlayProgressIndicatorPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.ToolBar?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="org.pcsoft.framework.jfex.data.ListViewExView">
    <children>
        <BorderPane fx:id="pnlList">
            <top>
                <ToolBar fx:id="toolbar" managed="${toolbar.visible}"/>
            </top>
            <center>
                <ListView fx:id="lst" maxWidth="1.7976931348623157E308" onMouseClicked="#onListMouseClick"/>
            </center>
        </BorderPane>
        <Label fx:id="lblEmpty" text="%cmp.data.empty" styleClass="text-empty"/>
        <OverlayProgressIndicatorPane fx:id="pnlProgress"/>
    </children>
</fx:root>
